[{
    "settings": [ "master" ],
		"_framed_view": {
		  "verbs": ["get"],
		  "path": "/",
		  "call": "layout.index"
		},
		"question index":{
			"verbs":["get"],
			"path":"/question/",
			"call":"q.index"
		},
		"question add":{
			"verbs":["post"],
			"path":"/question/add",
			"call":"q.add"
		},
		"question list":{
			"verbs":["get"],
			"path":"/question/list",
			"call":"q.list"
		},
		"question thanks":{
			"verbs":["get"],
			"path":"/question/thanks",
			"call":"q.thanks"
		}
}]
